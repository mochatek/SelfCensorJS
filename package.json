{
  "name": "self-censor",
  "version": "1.0.0",
  "description": "A JavaScript library that provides a simple solution for censoring segments of video in the browser. Define different censor tracks based on categories and switch them on-the-fly.",
  "author": "Akash S Panickar",
  "license": "MIT",
  "homepage": "https://github.com/mochatek",
  "repository": {
    "type": "git",
    "url": "https://github.com/mochatek/SelfCensorJS.git"
  },
  "bugs": {
    "url": "https://github.com/mochatek/SelfCensorJS/issues"
  },
  "keywords": ["movie censoring", "video censoring", "custom censoring", "HTML video plugin"],
  "type": "module",
  "main": "./dist/self-censor.umd.cjs",
  "module": "./dist/self-censor.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/self-censor.js",
      "require": "./dist/self-censor.umd.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-plugin-dts": "^1.7.2"
  }
}
